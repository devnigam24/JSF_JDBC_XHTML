INSERT INTO USERSPOST(USERNAME,POSTS) VALUES('CC','KEJRIVAAL IS BB');
INSERT INTO USERSPOST(USERNAME,POSTS) VALUES('CC','KEJRIVAAL IS MAHADON');

CREATE TABLE IF NOT EXISTS USERSCOMMENTS (POSTS VARCHAR(150) NOT NULL,COMMENTS VARCHAR(100) NOT NULL,COMMENTEDUSER VARCHAR(50),PRIMARY KEY(POSTS,COMMENTS));

