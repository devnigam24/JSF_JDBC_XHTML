CREATE TABLE  IF NOT EXISTS ALLUSERS(USERNAME VARCHAR(50) NOT NULL PRIMARY KEY,PASSWORD VARCHAR(50) NOT NULL);

INSERT INTO ALLUSERS(USERNAME,PASSWORD) VALUES('DEV','DEV');
INSERT INTO ALLUSERS(USERNAME,PASSWORD) VALUES('CC','CC');
INSERT INTO ALLUSERS(USERNAME,PASSWORD) VALUES('NIMMO','NIMMO');


CREATE TABLE IF NOT EXISTS USERSPOST (USERNAME VARCHAR(50) NOT NULL,POSTS VARCHAR(150) NOT NULL, PRIMARY KEY(USERNAME,POSTS));

INSERT INTO USERSPOST(USERNAME,POSTS) VALUES('DEV','KEJRIVAAL IS DON');
